<?xml version='1.0' encoding='utf-8'?>
<widget id="de.teemachen.teeuhr" version="1.1.7" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <description>
        A basic Framework7 template for PhoneGap.
    </description>
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <name>TeeUhr</name>
    <description>
        Die ultimative Teeuhr um jede Teesorte optimal zuzubereiten. 
    </description>
    <author email="support@teemachen.de" href="http://www.teemachen.de">TeeMachen Team</author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon src="icon.png" />
    <preference name="DisallowOverscroll" value="true" />
</widget>
